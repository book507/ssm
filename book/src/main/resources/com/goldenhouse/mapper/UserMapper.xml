<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goldenhouse.mapper.UserMapper">
    <insert id="registerUser" parameterType="user">
            <![CDATA[
                                      insert into
                                      t_user(username,password,repassword)
                                      values(#{username},MD5(#{password}),MD5(#{repassword}))
        ]]>
    </insert>
</mapper>